{
  "manifest_version": 2,

  "name": "GoJIRA",
  "description": "Transform JIRA issues from text to links.",
  "version": "0.1",
  "options_page": "options.html",

  "permissions": ["storage"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.1.min.js", "gojira.js"]
    }
  ]
}
